{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/static/images/*.{png,jpg,gif,svg,webp}",
            "/static/css/*",
            "/static/js/*",
            "/assets/*",
            "/api/*",
            "*.js",
            "*.css",
            "*.json",
            "*.ico",
            "*.png",
            "*.jpg",
            "*.gif",
            "*.svg",
            "*.webp"
        ]
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "application/javascript",
        ".css": "text/css"
    },
    "globalHeaders": {
        "Cache-Control": "no-cache"
    },
    "routes": [
        {
            "route": "/assets/*",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "*.js",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "*.css",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200
        }
    ]
}